{
  "name": "suncore-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "test": "jest --runInBand",
    "prepare": "husky install",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "format": "prettier --write ."
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --",
      "prettier --write --"
    ],
    "*.{json,md}": [
      "prettier --write --"
    ]
  },
  "dependencies": {
    "@sentry/node": "^7.103.0",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.6.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "micro": "^10.0.1",
    "mongoose": "^8.0.3",
    "querystring": "^0.2.1",
    "stripe": "^18.2.1",
    "supertest": "^7.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0"
