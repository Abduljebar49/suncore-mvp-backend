  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "eslint": "8",
    "eslint-plugin-node": "11",
    "husky": "8.0.3",
    "jest": "^30.0.4",
    "lint-staged": "13.2.3",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.0.2",
    "prettier": "^3.6.2"
  }
}
